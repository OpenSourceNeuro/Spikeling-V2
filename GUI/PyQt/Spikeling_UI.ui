                                <item alignment="Qt::AlignTop">
                                 <widget class="QTextBrowser" name="NeuronGenerator_subframe1_Izhik_textbrowser">
                                  <property name="sizePolicy">
                                   <sizepolicy hsizetype="Expanding" vsizetype="Fixed">
                                    <horstretch>0</horstretch>
